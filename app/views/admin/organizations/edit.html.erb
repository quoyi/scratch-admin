<% content_for :breadcrumbs do %>
  <li class="breadcrumb-item"><%= link_to t('organizations'), [:admin, :organizations] %></li>
  <li class="breadcrumb-item active"><%= t('edit') %></li>
<% end %>
<% content_for :navbars do %>
  <%= link_to t('show'), [:admin, @organization], class: 'btn btn-sm btn-primary mt-n2 mb-n1' %>
  <%= link_to t('back'), [:admin, :organizations], class: 'btn btn-sm btn-info mt-n2 mb-n1' %>
<% end %>

<div class="card">
  <div class="card-body">
    <%= render 'form', organization: @organization %>
  </div>
</div>
