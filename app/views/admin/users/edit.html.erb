<% content_for :breadcrumbs do %>
  <li class="breadcrumb-item"><%= link_to t('admin.users'), [:admin, :users] %></li>
  <li class="breadcrumb-item active"><%= t('edit') %></li>
<% end %>
<% content_for :navbars do %>
  <%= link_to t('show'), [:admin, @user], class: 'btn btn-sm btn-primary' %>
  <%= link_to t('back'), [:admin, :users], class: 'btn btn-sm btn-info' %>
<% end %>

<div class="card">
  <div class="card-body">
    <%= render 'form', user: @user %>
  </div>
</div>
